{
  "name": "hikolang",
  "version": "0.1",
  "description": "A compiler for the Hiko-language, a system programming language.",
  "homepage": "https://github.com/hikoworks/hikolang",
  "license": "BSL-1.0",
  "supports": "windows | linux | osx",
  "dependencies": [
    {
      "name": "icu"
    },
    {
      "name": "ms-gsl"
    },
    {
      "name": "libgit2",
      "features": [ "ssh" ]
    },
    {
      "name": "openssl"
    },
    {
      "name": "llvm",
      "default-features": false,
      "features": ["default-targets"]
    }
  ]
}
